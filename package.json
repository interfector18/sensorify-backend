{
  "name": "sensorify",
  "version": "1.0.0",
  "description": "",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run build && tsc -p test/tsconfig.test.json && cross-env TS_NODE_FILES=true tap --ts test/**/*.test.ts",
    "start": "npm run build && node dist/app.js",
    "build": "tsc",
    "dev": "tsc && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"tsc -w\" \"fastify start --ignore-watch=.ts$ -w -l info -p 8089 -P dist/app.js\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "2.26.0",
    "fastify": "^3.0.0",
    "fastify-autoload": "^3.3.1",
    "fastify-cli": "^2.13.0",
    "fastify-plugin": "^3.0.0",
    "fastify-sensible": "^3.1.0"
  },
  "devDependencies": {
    "@types/node": "^15.0.0",
    "@types/tap": "^15.0.0",
    "concurrently": "^6.0.0",
    "cross-env": "^7.0.3",
    "fastify-tsconfig": "^1.0.1",
    "prisma": "2.26.0",
    "tap": "^15.0.9",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5"
  }
}
